        <div class="login-form">
          <div class="login-agileits-top">
            <form>
              <p>Username</p>
              <input
                id="inputUsername"
                type="text"
                class="name"
                name="username"
                placeholder="Username"
                required=""
              />
              <p>Password</p>
              <input
                id="inputPassword"
                type="password"
                class="password"
                name="Password"
                placeholder=".........."
                required=""
              />

              <input id="submitButton" type="submit" />
            </form>
          </div>
        </div>
      </div>
    </div>
