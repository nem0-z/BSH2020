<!DOCTYPE html>
<html>
  <title>ShowSolution</title>
  <meta name="viewport" content="width=device-width, initial-scale=1" />

  <link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css" />
  <link rel="stylesheet" href="/css/showsolution.css" />
  <body>
    <div class="main">
    <div class="w3-card-4" style="width: 60%;"></div>
      <div class="w3-container "  >
        <h2 id="taskTitle"></h2>
      </div>
      <form class="w3-container">
        <h4 class="label">Username</h4>
        <p
            class="w3-input w3-border w3-white"
            id="username"
            type="text">
        </p>
   
        <h4 class="label">Comment</h4>
        <p
            class="w3-input w3-border w3-white"
            id="comment"
            type="text">
        </p>
        <h4 class="label">Solved on date</h4>
        <p
            class="w3-input w3-border w3-white"
            id="dateSolved"
            type="text">
        </p>
        <p style="text-align: center">
          <button
            class="w3-btn "
            id="returnBtn"
            type="submit">
            Return
          </button>
        </p>
      </form>
    </div>
  </div>
    <script src="../showsolution.js"></script>
    <script src="../sendHttpRequest.js"></script>
  </body>
</html>

